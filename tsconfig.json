{
  "allowJs": true,
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true
  },
  "noEmit": true,
  "target": "ESNext",
  "jsx": "react-native",
  "moduleResolution": "node",
  "resolveJsonModule": true,
  "esModuleInterop": true,
  "lib": ["DOM", "ESNext"],
  "skipLibCheck": true
}
